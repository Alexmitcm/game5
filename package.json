{
  "name": "hey",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "build": "echo 'This is a monorepo - use apps/api for Railway deployment'",
    "codegen": "pnpm --recursive --parallel run codegen",
    "dep:check": "pnpm dlx sherif@latest",
    "dep:fix": "pnpm dlx sherif@latest --fix",
    "dev": "pnpm --recursive --parallel run dev",
    "prepare": "husky install",
    "start": "echo 'This is a monorepo - use apps/api for Railway deployment'",
    "test": "pnpm --recursive --parallel run test",
    "typecheck": "pnpm --recursive --parallel run typecheck",

  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "husky": "^9.1.7",
    "supabase": "^2.33.7"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=18.x <=22.x"
  }
}
